@model FoodDonation.Models.UserReport

@{
    ViewData["Title"] = "Report";
}
<section class="container">

    <h1 class="text-center">Details</h1>

    <div>
        <h4 class="text-center">UserReport</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.UserName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.UserName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.reportDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.reportDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Description)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Description)
            </dd>
            <dt class="col-sm-2">
               Category
            </dt>
            <dd class="col-sm-10">
                @ViewBag.Category
            </dd>
        </dl>
    </div>
    <div>
        <a asp-controller="admin" asp-action="Notifications">Back to Notification</a>
    </div>

</section>